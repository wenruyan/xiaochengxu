}
.next-box {
  /* width: 368rpx; */
  margin-left: 20rpx;
  text-align: left;
  height: 160rpx;
}
.title {
  color: #3F454B;
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 24rpx;
}
.dis-time {
  margin-top: 14rpx;
}
.subtitle {
  color: #86939F;
  font-size: 24rpx;
}
.time {
  font-size: 24rpx;
  color: #86939F;
  margin-top: 14rpx;
}
.pass-image {
  position: absolute;
  width: 108rpx;
  height: 111rpx;
  bottom: 0;
  right: 0;
}
.modal {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 999999;
}

.modal-content {
    height: 388rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 620rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.modal-content .title {
    color: #1D2023;
    font-size: 40rpx;
    /* margin: 0 auto; */
    text-align: center;
    margin: 72rpx 0 22rpx 0;
}

.modal-content .tip {
    color: #606972;
    font-size: 30rpx;
    margin: 0 auto;
    text-align: center;
    width: 432rpx;
}

.modal .modal-bottom {
    position: absolute;
    bottom: 0;
    width: 100%;
    font-size: 0;
    display: flex;
}

.modal .modal-bottom .text {
    display: inline-block;
    width: 50%;
    height: 116rpx;
    line-height: 116rpx;
    text-align: center;
    font-size: 32rpx;
    border-top: 1px solid #e6e6e6;
    box-sizing: border-box;
}

.modal .modal-bottom .cancel {
    color: #86939f;
    height: 116rpx;
}

.modal .modal-bottom .confirm {
    color: #ff6600;
    border-left: 1px solid #e6e6e6;
    height: 116rpx;
}
.no-record {
  width: 100%;
  height: 100%;
  text-align: center;
  padding-top: 206rpx;
  box-sizing: border-box;
  color: #707070;
  font-size: 28rpx;
  background-color: #F2F2F2; 
}
.mony-1 {
  font-size: 36rpx;
}
.mony-2 {
  font-size: 16rpx;
  transform: scale(0.5)
}
.text-1 {
  font-size: 48rpx;
}
.text-2 {
