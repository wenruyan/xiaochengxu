}

.loading .item {
  width: 670rpx;
  margin: 20rpx auto 0;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
}

.loading .item .pic {
  height: 140rpx;
  width: 140rpx;
  background-color: #f2f2f2;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.loading .item .line {
  height: 50rpx;
  width: 350rpx;
  background-color: #f2f2f2;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  border-radius: 5rpx;
}

.loading .item .line:last-child {
  width: 450rpx;
}