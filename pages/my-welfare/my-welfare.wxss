page {
  height: auto;
}

.container {
  background-color: #fff;
}

.main-content {
  text-align: center;
}
.select-bar {
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  line-height: 40rpx;
}
.select-bar .item {
  text-align: center;
  width: 33.33%;
  float: left;
  font-size: 28rpx;
  color: #606972;
  font-weight: bold;
  position: relative;
}
.select-bar .item.active {
  color: #1D2023;
}
.select-bar .item .line {
  width: 0;
  height: 4rpx;
  background-color: #FF6600;
  margin: 0 auto;
  margin-top: 26rpx;
  transition: width 0.2s;
}
.select-bar .item.active .line {
  width: 100rpx;
}
.scroll {
  height: 100%;
  width: 100%;
  background-color: #F2F2F2;
}
.list {
  height: 100%;
  width: 100%;
  background-color: #F2F2F2;
  padding: 30rpx;
  box-sizing: border-box;
}
.list-detail {
  /* width: 690rpx; */
  height: 184rpx;
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 16rpx;
  box-shadow:0px 4px 6px rgba(206,141,94,0.02);
  box-sizing: border-box;
  border-radius: 20rpx;
  position: relative;
}
.list-detail .btn {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;  
}

.list-detail1 {
  margin-bottom: 0rpx;
  border-radius: 0;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}
.time-box {
 position: absolute;
  width: 40rpx;
  height: 36rpx;
  top: 10rpx;
  right: 10rpx; 
}
.time-image {
  width: 40rpx;
  height: 36rpx;
}
.box {
  display: flex
}
.list-box {
  width: 140rpx;
  height: 140rpx;
  position: relative;
}
.price {
