page {
  height: auto;
}

.container {
  background-color: #fff;
  height: 100%;
}

.main-content {
  text-align: center;
  background-color: #fff;
  overflow: hidden;
}

.content {
  position: relative;
  height: calc(100vh - 44px - 20px);
  /* padding: 0 45rpx; */
  text-align: left;
  background-color: #FAFAFA;
  overflow: auto;
}
.fuli-image {
  width: 100%;
  height: 432rpx;
}
.image-box {
  position: relative;
  width: 100%;
  height: 432rpx;
}
.text-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 432rpx;
  padding-top: 120rpx;
  padding-left: 64rpx;
  box-sizing: border-box;
}
.text-box .text {
  color: #FEFEFE;
  font-size: 58rpx;
  font-weight: bold;
}
.text-box .text1 {
  color: #FEFEFE;
  font-size: 26rpx;
  margin-top: 20rpx;
}
.form {
  margin: 0 auto;
  width: 648rpx;
  height: 1087rpx;
  box-shadow:1px 10px 30px 0px rgba(81,80,255,0.1);
  border-radius:10px;
  top: -114rpx;
  position: relative;
  background-color: #fff;
  padding: 96rpx 50rpx;
  box-sizing: border-box;
}
.form .title {
  color: #121727;
  font-size: 26rpx;
  font-weight: bold;
  padding-left: 30rpx;
}
.input-wrap {
  /* height: 96rpx;
  line-height: 96rpx; */
  padding: 22rpx 0 22rpx 22rpx;
  border-radius: 10rpx;
  background-color: #F5F5F5;
  margin-top: 26rpx;
  margin-bottom: 30rpx;
}
.input-wrap input {
  color: #6C6F7A;
  font-size: 26rpx;
}
.agree {
  display: flex;
  font-size: 24rpx;
  color: #999;
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
  margin-bottom: 28rpx;
}
.blue {
  color: #5251FF;
}
.no-image {
  width: 22rpx;
  height: 22rpx;
  margin-right: 13rpx;
}
.apply {
  background-color: #FF6600;
  height: 86rpx;
  line-height: 86rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 60rpx;
  box-sizing: border-box;
}
.star-text {
  color: #F71706;
}