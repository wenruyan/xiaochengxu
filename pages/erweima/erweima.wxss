.box {
    background: rgba(241, 241, 241, 1);
    min-height: 100%;
}

.topContent {
    height: 290rpx;
    background: #FFCC35;
    padding: 0 30rpx;
    padding-top: 158rpx;
    display: flex;
}

.topbg {
    background: #FFCC35 !important;
}

.topContent image {
    width: 180rpx;
    height: 180rpx;
    border: 4rpx solid #fff;
    box-sizing: border-box;
    border-radius: 10rpx;
    margin-right: 22rpx;
}

.topContent .skuDesc {
    flex: 1;
}

.topContent .skuDesc .skuTitle {
    height: 42rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #5B3E00;
}


.topContent .skuDesc .simpleDesc .desc {
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    line-height: 45rpx;
    color: rgba(138, 94, 0, 1);
}

.topbg .skuDesc .simpleDesc .desc {
    color: #8A5E00 !important;
}

.mainContent {
    width: 630rpx;
    margin: 0 auto;
    background: rgba(255, 255, 255, 1);
    border-radius: 10rpx;
    position: relative;
    top: -60rpx;
    padding: 52rpx 30rpx 30rpx 30rpx;
}

.mainContent>image {
    width: 334rpx;
    height: 334rpx;
    position: absolute;
    right: 0;
    top: 0;
}

.mainContent .title {
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    margin-bottom: 40rpx;
    display: flex;
    justify-content: space-between;
    height: 50rpx;
    align-items: center;
}
.mainContent .check-used {
    width: 226rpx;
    height: 226rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.use-btn {
    position: absolute;
    top: 46rpx;
    right: 24rpx;
    width: 154rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
    border: 1px solid #ff6600;
    color: #ff6600;
    border-radius: 6rpx;
}

.mainContent .code {
    display: flex;
    flex-direction: column;
    /* height: 400rpx; */
    align-items: center;
    justify-content: space-between;
    margin-bottom: 30rpx;
}

.mainContent .code .codeImg {
    position: relative;
}

.mainContent .code .codeImg image {
    width: 244rpx;
    height: 244rpx;
}

.mainContent .code .codeImg>view {
    width: 244rpx;
    height: 244rpx;
    background: rgba(255, 255, 255, .6);
    position: absolute;
    left: 0;
    top: 0;
}

.mainContent .code .codeImg>view view {
    width: 179rpx;
    height: 48rpx;
    background: rgba(0, 0, 0, 0.4);
    border: 2px solid rgba(255, 255, 255, 1);
    border-radius: 6px;
    line-height: 48rpx;
    text-align: center;
    font-size: 20rpx;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
    margin: 100rpx auto;
}

.mainContent .code .slogan {
    font-size: 26rpx;
    color: rgba(134, 147, 159, 1);
}

.mainContent .code .open {
    width: 380rpx;
    height: 80rpx;
    background: rgba(255, 102, 0, 1);
    border-radius: 60px;
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
    margin-top: 20rpx;
}

.mainContent .endTime {
    height: 72rpx;
    background: rgba(255, 245, 214, 1);
    border-radius: 10px;
    font-size: 24rpx;
    color: rgba(72, 55, 3, 1);
    text-align: center;
    line-height: 72rpx;
    margin-bottom: 34rpx;
}


.mainContent .description view {
    font-size: 24rpx;
    color: rgba(96, 105, 114, 1);
    text-align: justify;
    line-height: 34rpx;
    margin: 10rpx 0;
}

.mainContent .description .contact {
    display: flex;
    align-items: center
}

.mainContent .description .contact view {
    position: relative;
    color: #2454F5;
}

.mainContent .description .contact view button {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.codeModal {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.4);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.codeModal .modalContent {
    width: 620rpx;
    height: 422rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    margin-bottom: 60rpx;
}

.codeModal .modalContent .confirmTitle {
    font-size: 40rpx;
    font-weight: bold;
    color: rgba(29, 32, 35, 1);
}

.codeModal .modalContent .confirmSlogan {
    width: 452rpx;
    height: 108rpx;
    font-size: 30rpx;
    color: rgba(96, 105, 114, 1);
}

.codeModal .modalContent .confirmBtn {
    width: 522rpx;
    height: 90rpx;
    background: rgba(255, 102, 0, 1);
    border-radius: 60px;
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
}

.codeModal image {
    width: 66rpx;
    height: 66rpx;
}

.modal {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.6);
    z-index: 1;
}

.modal-content {
    height: 388rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 620rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.modal-content .title {
    color: #1D2023;
    font-size: 40rpx;
    margin: 0 auto;
    text-align: center;
    margin: 72rpx 0 22rpx 0;
}

.modal-content .tip {
    color: #606972;
    font-size: 30rpx;
    margin: 0 auto;
    text-align: center;
    width: 432rpx;
}

.modal .modal-bottom {
    position: absolute;
    bottom: 0;
    width: 100%;
    font-size: 0;
}

.modal .modal-bottom text {
    display: inline-block;
    width: 50%;
    height: 116rpx;
    line-height: 116rpx;
    text-align: center;
    font-size: 32rpx;
    border-top: 1px solid #e6e6e6;
    box-sizing: border-box;
}

.modal .modal-bottom text.cancel {
    color: #86939f;
}

.modal .modal-bottom text.confirm {
    color: #ff6600;
    border-left: 1px solid #e6e6e6;
}
