/* components/navbar.wxss */

.navbar {
  width: 750rpx;
  /* height: 88rpx;
  line-height: 88rpx; */
  position: fixed;
  top: 0;
  left: 0;
  overflow: hidden;
  text-align: center;
  /* background-color: rgba(0, 0, 0, 0); *//* border-bottom: 2rpx solid #E8E6E6; */
  z-index: 9;
  /* background-color: #F8F8F8; */
}

.navbar .bg {
  width: 100%;
  height: 88rpx;
  /* background-color: #000; *//* position: absolute;
  top: 0;
  left: 0; */
  z-index: -1;
}

.navbar .icon-back {
  padding: 28.5rpx 40rpx;
  width: 18rpx;
  height: 32rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.navbar .icon-back.white-bg {
  padding: 12.5rpx 30rpx;
  width: 64rpx;
  height: 64rpx;
}

.navbar .icon-back .icon-back-image {
  width: 100%;
  height: 100%;
}

.navbar .icon-back-text {
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  position: absolute;
  height: 88rpx;
  line-height: 88rpx;
  top: 0;
  left: 70rpx;
  z-index: 1;
}

.navbar .page-title {
  font-size: 36rpx;
  /* font-size: 30rpx; */
  color: #333;
  font-weight: bold;
  height: 88rpx;
  line-height: 88rpx;
  position: absolute;
  left: 0;
  right: 0;
}
