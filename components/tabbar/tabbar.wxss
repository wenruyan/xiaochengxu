.tabbar_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 98rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}

.tabbar_box.iphoneX-height {
    padding-bottom: 66rpx;
}

.middle-wrapper {
    position: absolute;
    right: 310rpx;
    bottom: 0;
    background-color: #fff;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
}

.middle-wrapper.iphoneX-height {
    bottom: 66rpx;
}

.tabbar_nav {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
    height: 100%;
    position: relative;
}

.tabbar_icon {
    width: 45rpx;
    height: 45rpx;
}

.special-wrapper {
    position: absolute;
    left: 77rpx;
    top: -36rpx;
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
    background-color: #fff;
    text-align: center;
    box-sizing: border-box;
    padding: 6rpx;
}

.special-wrapper .tabbar_icon {
    width: 84rpx;
    height: 84rpx;
}

.special-text-wrapper {
    width: 56rpx;
    height: 56rpx;
}

.confirm-tips {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 9999;
    text-align: center;
}

.confirm-tips .bg {
    width: 100%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background-color: #000000;
    opacity: 0.5;
    z-index: -1;
}

.confirm-tips .content {
    width: 580rpx;
    padding-bottom: 74rpx;
    background-color: #FFFFFF;
    border-radius: 25rpx;
    margin: 316rpx auto 128rpx;
    position: relative;
    color: #333333;
    text-align: center;
}

.confirm-tips .content .text-title {
    font-size: 46rpx;
    padding: 66rpx 0 50rpx;
    line-height: 1;
    font-weight: bold;
}

.confirm-tips .content .text-content {
    font-size: 30rpx;
    padding: 0 38rpx;
    line-height: 1.8;
    text-align: left;
    white-space: normal;
    word-break: break-all;
}

.confirm-tips .content .btn {
    width: 378rpx;
    margin-top: 65rpx;
}

.confirm-tips .close image {
    width: 62rpx;
    height: 62rpx;
}