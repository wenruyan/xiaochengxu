/**app.wxss**/
/** 全局样式 **/
@font-face {
  font-family: 'DIN Pro Cond';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
      url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
      url('DINPro-CondensedBold.ttf') format('truetype');
  font-weight: bold;
  font-style: normal;
}
page {
  width: 100%;
  height: 100%;
}
.i-modal-mask-show {
  opacity: 0.5;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}

.wrap {
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
}

.wrapImg {
  position: absolute;
  top: 0;
}

.f-l {
  float: left;
}

.f-r {
  float: right;
}

.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.flex-v {
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  flex-direction: column;
}

.flex-align-center {
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.flex-pack-center {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.flex-pack-justify {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

.flex-1 {
  flex: 1;
}

.btn.btn-ht {
  width: 670rpx;
  margin: 0 auto;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
  font-weight: bold;
  border-radius: 10rpx;
  background-color: #FF2B44;
  color: #FFFFFF;
}
.btn.btn-right {
  width:320rpx;
  height:90rpx;
  background:rgba(255,177,186,1);
  opacity:1;
  border-radius:10rpx;
}

.main-content {
  /* overflow-y: auto; */
  /* -webkit-overflow-scrolling: touch; */
  box-sizing: content-box;
}

.tui-countdown-content{
  height: 32rpx;
  line-height: 32rpx;
  text-align: left;
  font-size: 18rpx;
  color: #FF2B44;
  margin-top: 18rpx;
}
/* 倒计时 */
.tui-countdown-content .txt {
  font-size: 20rpx;
  color: #999;
  margin-right: 24rpx;
}
.tui-conutdown-box{
  display: inline-block;
  height: 32rpx;
  width: 32rpx;
  line-height: 32rpx;
  text-align: center;
  background-color: #FF2B44;
  color: #fff;
  border-radius:4rpx;
  margin-right: 9rpx;
}
.tui-countdown-bg{
  background-color: #FF2B44;
}
/* 垂直水平居中 */
.tvac {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
/* 水平居中 */
.tac {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
/* 垂直居中 */
.tvc {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
button::after{
  width: 0;
  height: 0;
  border: 0;
}

.ov-h {
  overflow: hidden;
}